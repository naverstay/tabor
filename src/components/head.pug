head
  meta(charset='utf-8')
  meta(http-equiv='x-ua-compatible', content='ie=edge')
  meta(name='viewport', content='width=device-width, initial-scale=1.0')
  meta(content='telephone=no', name='format-detection')
  meta(name='HandheldFriendly', content='true')

  meta(name='format-detection' content='telephone=no')
  meta(name='format-detection' content='address=no')

  meta(name='google' content='notranslate')

  meta(name='apple-mobile-web-app-capable' content='yes')
  meta(name='apple-mobile-web-app-status-bar-style' content='black-translucent')

  meta(name='mobile-web-app-capable' content='yes')

  block head

  title= pageTitle || ''

  meta(name='description', content= pageDescription || '')

  <link rel="apple-touch-icon" sizes="180x180" href="./favicon/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="./favicon/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="./favicon/favicon-16x16.png">
  <link rel="manifest" href="./favicon/site.webmanifest">
  <link rel="mask-icon" href="./favicon/safari-pinned-tab.svg" color="#5bbad5">
  <meta name="msapplication-TileColor" content="#da532c">
  <meta name="theme-color" content="#43b088">

  style#remove-styles.
    footer,
    footer ~ div,
    #section-1 ~ section,
    .slider--reg {
      display: none !important;
    }
    #section-1 {
      flex: 1 0 auto;
    }
    #section-1.section--holder.__end:before {
      border-bottom-right-radius: 0;
    }

  link(rel="stylesheet" id="inline-styles" href="./assets/scss/head.scss")

  //-script(src="https://cdnjs.cloudflare.com/ajax/libs/webfont/1.6.28/webfontloader.js")

  //-script.
    var fallbackTimeout = setTimeout(function () {
      document.documentElement.classList.add('__use-local-font');
      document.documentElement.style.visibility = 'visible';
    }, 5000);

    WebFont.load({
      google: {
        families: [
          "Roboto:100,300,400,500,700,900",
          "Ubuntu:400,500",
